               <ref name="tei_head"/>
            </zeroOrMore>
            <oneOrMore>
               <ref name="tei_orgName"/>
            </oneOrMore>
            <zeroOrMore>
               <ref name="tei_event"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="tei_idno"/>
            </zeroOrMore>
            <optional>
               <ref name="tei_desc"/>
            </optional>
            <optional>
