                context="tei:*[@subtype]">
         <sch:assert test="@type">The <sch:name/> element should not be categorized in detail with @subtype unless also categorized in general with @type</sch:assert>
      </sch:rule>
   </pattern>
   <define name="tei_include">
      <element name="include" ns="http://www.w3.org/2001/XInclude">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">is an element from the XML namespace of the <a xmlns="http://www.w3.org/1999/xhtml"
               href="https://www.w3.org/TR/xinclude-11/">XML Inclusions (XInclude)</a> W3C recommendation. It is used to include, into a ParlaMint <code xmlns="http://www.w3.org/1999/xhtml">&lt;teiCorpus&gt;</code> root file the elements of the corpus that are stored as separate files. These are the <code xmlns="http://www.w3.org/1999/xhtml">&lt;TEI&gt;</code> corpus components and parts of the corpus root <code xmlns="http://www.w3.org/1999/xhtml">&lt;teiHeader&gt;</code>. Inside <code xmlns="http://www.w3.org/1999/xhtml">&lt;particDesc&gt;</code> these are <code xmlns="http://www.w3.org/1999/xhtml">&lt;listPerson&gt;</code> &amp; <code xmlns="http://www.w3.org/1999/xhtml">&lt;listOrg&gt;</code>, and <code xmlns="http://www.w3.org/1999/xhtml">&lt;taxonomy&gt;</code> inside <code xmlns="http://www.w3.org/1999/xhtml">&lt;classDecl&gt;</code>.</a:documentation>
         <empty/>
         <optional>
            <attribute name="href">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"/>
               <data type="anyURI">
                  <param name="pattern">\S+</param>
               </data>
            </attribute>
         </optional>
         <empty/>
      </element>
   </define>
   <start>
      <choice>
         <ref name="tei_TEI"/>
         <ref name="tei_teiCorpus"/>
      </choice>
   </start>
</grammar>
