<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-parla.legislature"
          xml:lang="mul">
   <desc xml:lang="lt">
      <term>Įstatymų leidžiamoji valdžia (parlamentas)</term>
   </desc>
   <desc xml:lang="en">
      <term>Legislature</term>
   </desc>
   <category xml:id="parla.geo-political">
      <catDesc xml:lang="lt">
         <term>Geopolitiniai ar administraciniai vienetai</term>
      </catDesc>
      <catDesc xml:lang="en">
         <term>Geo-political or administrative units</term>
      </catDesc>
      <category xml:id="parla.supranational">
         <catDesc xml:lang="lt">
            <term>Viršnacionalinis parlamentas</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>Supranational legislature</term>
         </catDesc>
      </category>
      <category xml:id="parla.national">
         <catDesc xml:lang="lt">
            <term>Nacionalinis parlamentas</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>National legislature</term>
         </catDesc>
      </category>
      <category xml:id="parla.local">
         <catDesc xml:lang="lt">
            <term>Vietinis parlamentas (taryba)</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>Local legislature</term>
         </catDesc>
      </category>
   </category>
   <category xml:id="parla.organization">
      <catDesc xml:lang="lt">
         <term>Organizacija</term>
      </catDesc>
      <catDesc xml:lang="en">
         <term>Organization</term>
      </catDesc>
      <category xml:id="parla.chambers">
         <catDesc xml:lang="lt">
            <term>Rūmai</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>Chambers</term>
         </catDesc>
         <category xml:id="parla.uni">
            <catDesc xml:lang="lt">
               <term>Vienų rūmų parlamentas</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Unicameralism</term>
            </catDesc>
         </category>
         <category xml:id="parla.bi">
            <catDesc xml:lang="lt">
               <term>Dviejų rūmų parlamentas</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Bicameralism</term>
            </catDesc>
            <category xml:id="parla.upper">
               <catDesc xml:lang="lt">
                  <term>Aukštesnieji rūmai</term>
               </catDesc>
               <catDesc xml:lang="en">
                  <term>Upper house</term>
               </catDesc>
            </category>
            <category xml:id="parla.lower">
               <catDesc xml:lang="lt">
                  <term>Žemesnieji rūmai</term>
               </catDesc>
               <catDesc xml:lang="en">
                  <term>Lower house</term>
               </catDesc>
            </category>
         </category>
         <category xml:id="parla.multi">
            <catDesc xml:lang="lt">
               <term>Daugelio rūmų parlamentas</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Multicameralism</term>
            </catDesc>
            <category xml:id="parla.chamber">
               <catDesc xml:lang="lt">
                  <term>Rūmai</term>
               </catDesc>
               <catDesc xml:lang="en">
                  <term>Chamber</term>
               </catDesc>
            </category>
         </category>
      </category>
      <category xml:id="parla.committee">
         <catDesc xml:lang="lt">
            <term>Komitetas arba komisija</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>Committee</term>
         </catDesc>
         <category xml:id="parla.committee.standing">
            <catDesc xml:lang="lt">
               <term>Nuolatinis komitetas arba komisija</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Standing committee</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.special">
            <catDesc xml:lang="lt">
               <term>Specialioji (tyrimo) komisija</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Special committee</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.inquiry">
            <catDesc xml:lang="lt">
               <term>Laikinoji (tyrimo) komisija</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Committee of inquiry</term>
            </catDesc>
         </category>
      </category>
   </category>
   <category xml:id="parla.term">
      <catDesc xml:lang="lt">
         <term>Parlamento kadencija</term>: laikotarpis tarp nacionalinių
                            parlamento rinkimų.</catDesc>
      <catDesc xml:lang="en">
         <term>Legislative period</term>: term of the parliament between general
                            elections.</catDesc>
      <category xml:id="parla.session">
         <catDesc xml:lang="lt">
            <term>Parlamento sesija</term>: laikotarpis, kurio metu parlamentas posėdžiauja įstatymų
                                leidybos tikslais. Paprastai, tokių smulkesnių laikotarpių būna du ar daugiau tarp
                                dviejų nacionalinių parlamento rinkimų. Sesija yra posėdžiai arba susietų posėdžių
                                grupė, skirta svarstyti tam tikrą programą, darbotvarkę, darbotvarkės punktą ar kitokį
                                iš anksto paskelbtą dalyką.</catDesc>
         <catDesc xml:lang="en">
            <term>Legislative session</term>: the period of time in which a legislature is convened
                                for purpose of lawmaking, usually being one of two or more smaller divisions of the
                                entire time between two elections. A session is a meeting or series of connected
                                meetings devoted to a single order of business, program, agenda, or announced purpose.</catDesc>
         <category xml:id="parla.meeting">
            <catDesc xml:lang="lt">
               <term>Posėdis</term>: kiekvienas posėdis gali būti atskira sesija arba posėdžių
                                    grupės sudedamoji dalis. Sesija ar posėdis gali trukti vieną ar daugiau dienų.</catDesc>
            <catDesc xml:lang="en">
               <term>Meeting</term>: each meeting may be a separate session or part of a group of
                                    meetings constituting a session. The session/meeting may take one or more days.</catDesc>
            <category xml:id="parla.meeting-types">
               <catDesc xml:lang="lt">
                  <term>Posėdžių tipai</term>
               </catDesc>
               <catDesc xml:lang="en">
                  <term>Types of meetings</term>
               </catDesc>
               <category xml:id="parla.meeting.regular">
                  <catDesc xml:lang="lt">
                     <term>Eilinis posėdis</term>
                  </catDesc>
                  <catDesc xml:lang="en">
                     <term>Regular meeting</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.special">
                  <catDesc xml:lang="lt">
                     <term>Specialusis posėdis</term>
                  </catDesc>
                  <catDesc xml:lang="en">
                     <term>Special meeting</term>
                  </catDesc>
                  <category xml:id="parla.meeting.extraordinary">
                     <catDesc xml:lang="lt">
                        <term>Nenumatytas ar neeilinis posėdis</term>
                     </catDesc>
                     <catDesc xml:lang="en">
                        <term>Extraordinary meeting</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.urgent">
                     <catDesc xml:lang="lt">
                        <term>Skubus posėdis</term>
                     </catDesc>
                     <catDesc xml:lang="en">
                        <term>Urgent meeting</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.ceremonial">
                     <catDesc xml:lang="lt">
                        <term>Iškilmingas posėdis</term>
                     </catDesc>
                     <catDesc xml:lang="en">
                        <term>Ceremonial meeting</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.commemorative">
                     <catDesc xml:lang="lt">
                        <term>Minėjimo posėdis</term>
                     </catDesc>
                     <catDesc xml:lang="en">
                        <term>Commemorative meeting</term>
                     </catDesc>
                  </category>
               </category>
               <category xml:id="parla.meeting.continued">
                  <catDesc xml:lang="lt">
                     <term>Posėdžio tęsinys</term>
                  </catDesc>
                  <catDesc xml:lang="en">
                     <term>Continued meeting</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.public">
                  <catDesc xml:lang="lt">
                     <term>Išvažiuojamasis posėdis</term>
                  </catDesc>
                  <catDesc xml:lang="en">
                     <term>Public meeting</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.executive">
                  <catDesc xml:lang="lt">
                     <term>Uždaras posėdis</term>
                  </catDesc>
                  <catDesc xml:lang="en">
                     <term>Executive meeting</term>
                  </catDesc>
               </category>
            </category>
         </category>
      </category>
   </category>
</taxonomy>
