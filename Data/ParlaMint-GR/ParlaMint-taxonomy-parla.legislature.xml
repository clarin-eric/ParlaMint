<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-parla.legislature"
          xml:lang="mul">
   <desc xml:lang="el">
      <term>Νομοθετικό σώμα</term>
   </desc>
   <desc xml:lang="en">
      <term>Legislature</term>
   </desc>
   <category xml:id="parla.geo-political">
      <catDesc xml:lang="el">
         <term>Γεωπολιτικές ή διοικητικές μονάδες</term>
      </catDesc>
      <catDesc xml:lang="en">
         <term>Geo-political or administrative units</term>
      </catDesc>
      <category xml:id="parla.national">
         <catDesc xml:lang="el">
            <term>Εθνικό νομοθετικό σώμα</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>National legislature</term>
         </catDesc>
      </category>
   </category>
   <category xml:id="parla.organization">
      <catDesc xml:lang="el">
         <term>Οργανισμός</term>
      </catDesc>
      <catDesc xml:lang="en">
         <term>Organization</term>
      </catDesc>
      <category xml:id="parla.chambers">
         <catDesc xml:lang="el">
            <term>Θάλαμοι</term>
         </catDesc>
         <catDesc xml:lang="en">
            <term>Chambers</term>
         </catDesc>
         <category xml:id="parla.uni">
            <catDesc xml:lang="el">
               <term>μονοθάλαμο</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>unicameral</term>
            </catDesc>
         </category>
      </category>
   </category>
   <category xml:id="parla.term">
      <catDesc xml:lang="el">
         <term>Κοινοβουλευτική περίοδος</term>:περίοδος μεταξύ βουλευτικών εκλογών.</catDesc>
      <catDesc xml:lang="en">
         <term>Legislative period</term>:term of the parliament between general elections.</catDesc>
      <category xml:id="parla.session">
         <catDesc xml:lang="el">
            <term>Σύνοδος</term>:μια χρονική περίοδος κατά την οποία συγκαλείται το νομοθετικό σώμα, συνήθως ένα έτος.</catDesc>
         <catDesc xml:lang="en">
            <term>Session</term>:a period of time in which a legislature is convened, typically one year.</catDesc>
         <category xml:id="parla.meeting">
            <catDesc xml:lang="el">
               <term>Συνάντηση</term>
            </catDesc>
            <catDesc xml:lang="en">
               <term>Meeting</term>:each meeting will be a constitute part of a parliamentary session.</catDesc>
            <category xml:id="parla.sitting">
               <catDesc xml:lang="el">
                  <term>Συνεδρίαση</term>
               </catDesc>
               <catDesc xml:lang="en">
                  <term>Sitting</term>:sitting day.</catDesc>
            </category>
         </category>
      </category>
   </category>
</taxonomy>
