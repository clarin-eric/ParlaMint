<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-parla.legislature"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Legislature</term>
   </desc>
   <category xml:id="parla.geo-political">
      <catDesc xml:lang="en">
         <term>Geo-political or administrative units</term>
      </catDesc>
      <catDesc xml:lang="tr">
         <term>Jeopolitik veya idari birimler</term>
      </catDesc>
      <category xml:id="parla.supranational">
         <catDesc xml:lang="en">
            <term>Supranational legislature</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Uluslarüstü yasama</term>
         </catDesc>
      </category>
      <category xml:id="parla.national">
         <catDesc xml:lang="en">
            <term>National legislature</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Ulusal yasama</term>
         </catDesc>
      </category>
      <category xml:id="parla.regional">
         <catDesc xml:lang="en">
            <term>Regional legislature</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Bolgesel yasama</term>
         </catDesc>
      </category>
      <category xml:id="parla.local">
         <catDesc xml:lang="en">
            <term>Local legislature</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Yerel yasama</term>
         </catDesc>
      </category>
   </category>
   <category xml:id="parla.organization">
      <catDesc xml:lang="en">
         <term>Organization</term>
      </catDesc>
      <catDesc xml:lang="tr">
         <term>Kurum</term>
      </catDesc>
      <category xml:id="parla.chambers">
         <catDesc xml:lang="en">
            <term>Chambers</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Meclisler</term>
         </catDesc>
         <category xml:id="parla.uni">
            <catDesc xml:lang="en">
               <term>Unicameralism</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Tek meclisli</term>
            </catDesc>
         </category>
         <category xml:id="parla.bi">
            <catDesc xml:lang="en">
               <term>Bicameralism</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Çift meclisli</term>
            </catDesc>
            <category xml:id="parla.upper">
               <catDesc xml:lang="en">
                  <term>Upper house</term>
               </catDesc>
               <catDesc xml:lang="tr">
                  <term>Üst meclis</term>
               </catDesc>
            </category>
            <category xml:id="parla.lower">
               <catDesc xml:lang="en">
                  <term>Lower house</term>
               </catDesc>
               <catDesc xml:lang="tr">
                  <term>Alt meclis</term>
               </catDesc>
            </category>
         </category>
         <category xml:id="parla.multi">
            <catDesc xml:lang="en">
               <term>Multicameralism</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Çok meclisli</term>
            </catDesc>
            <category xml:id="parla.chamber">
               <catDesc xml:lang="en">
                  <term>Chamber</term>
               </catDesc>
               <catDesc xml:lang="tr">
                  <term>Meclis</term>
               </catDesc>
            </category>
         </category>
      </category>
      <category xml:id="parla.committee">
         <catDesc xml:lang="en">
            <term>Committee</term>
         </catDesc>
         <catDesc xml:lang="tr">
            <term>Komite</term>
         </catDesc>
         <category xml:id="parla.committee.standing">
            <catDesc xml:lang="en">
               <term>Standing committee</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Daimi komisyon</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.special">
            <catDesc xml:lang="en">
               <term>Special committee</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Özel komisyon</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.inquiry">
            <catDesc xml:lang="en">
               <term>Committee of inquiry</term>
            </catDesc>
            <catDesc xml:lang="tr">
               <term>Araştırma komisyonu</term>
            </catDesc>
         </category>
      </category>
   </category>
   <category xml:id="parla.term">
      <catDesc xml:lang="en">
         <term>Legislative period</term>: term of the parliament between general elections.</catDesc>
      <catDesc xml:lang="tr">
         <term>Yasama dönemi</term>: Genel seçimler arası meclis donemi.</catDesc>
      <category xml:id="parla.session">
         <catDesc xml:lang="en">
            <term>Session</term>: A parliamentary year. The time period between the 1st of October and the 30th of September following year. Turkish parliamentary year is organised in such a way that it primarily takes place from October to June</catDesc>
         <catDesc xml:lang="tr">
            <term>Yasama yılı</term>: 1 Ekim'de başlayıp 30 Eylül'de sona eren süre. Yasama faaliyetleri normal kosullarda Ekim başından Temmuz sonuna kadar sürer.</catDesc>
         <category xml:id="parla.meeting">
            <catDesc xml:lang="en">
               <term>Meeting</term>: A day-long meeting of the main proceedings of the parliament.</catDesc>
            <catDesc xml:lang="tr">
               <term>Birleşim</term>: Genel Kurulunun belirli bir günde açılan toplantısıdır.</catDesc>
            <category xml:id="parla.sitting">
               <catDesc xml:lang="en">
                  <term>Sitting</term>: a parliamentary meeting without break. Usually one meeting constitutes multiple sittings.</catDesc>
               <catDesc xml:lang="tr">
                  <term>Oturum</term>: Aralıksız bir meclis toplantısı. Genellikle bir birleşim birkaç oturum içerir.</catDesc>
            </category>
            <category xml:id="parla.meeting-types">
               <catDesc xml:lang="en">
                  <term>Types of meetings</term>
               </catDesc>
               <catDesc xml:lang="tr">
                  <term>Toplantı türleri</term>
               </catDesc>
               <category xml:id="parla.meeting.regular">
                  <catDesc xml:lang="en">
                     <term>Regular meeting</term>
                  </catDesc>
                  <catDesc xml:lang="tr">
                     <term>Normal toplantı</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.special">
                  <catDesc xml:lang="en">
                     <term>Special meeting</term>
                  </catDesc>
                  <catDesc xml:lang="tr">
                     <term>Özel toplantı</term>
                  </catDesc>
                  <category xml:id="parla.meeting.extraordinary">
                     <catDesc xml:lang="en">
                        <term>Extraordinary meeting</term>
                     </catDesc>
                     <catDesc xml:lang="tr">
                        <term>Olağanüstü toplantı</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.urgent">
                     <catDesc xml:lang="en">
                        <term>Urgent meeting</term>
                     </catDesc>
                     <catDesc xml:lang="tr">
                        <term>Acil toplantı</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.ceremonial">
                     <catDesc xml:lang="en">
                        <term>Ceremonial meeting</term>
                     </catDesc>
                     <catDesc xml:lang="tr">
                        <term>Kutlama toplantısı</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.commemorative">
                     <catDesc xml:lang="en">
                        <term>Commemorative meeting</term>
                     </catDesc>
                     <catDesc xml:lang="tr">
                        <term>Anma toplantısı</term>
                     </catDesc>
                  </category>
               </category>
               <category xml:id="parla.meeting.continued">
                  <catDesc xml:lang="en">
                     <term>Continued meeting</term>
                  </catDesc>
                  <catDesc xml:lang="tr">
                     <term>Devam toplantısı</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.public">
                  <catDesc xml:lang="en">
                     <term>Public meeting</term>
                  </catDesc>
                  <catDesc xml:lang="tr">
                     <term>Kamuya açık toplantı</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.executive">
                  <catDesc xml:lang="en">
                     <term>Executive meeting</term>
                  </catDesc>
                  <catDesc xml:lang="tr">
                     <term>Yönetici toplantısı</term>
                  </catDesc>
               </category>
            </category>
         </category>
      </category>
   </category>
</taxonomy>
