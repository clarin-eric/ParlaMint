Subcorpusurion="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-subcorpus"
          xml:lang="mul">
   <desc xml:lang="ro">
      <term>Subcorpusuri</term>
   </desc>
   <desc xml:lang="en">
      <term>Subcorpora</term>
   </desc>
   <category xml:id="reference">
      <catDesc xml:lang="ro">
         <term>Referință</term>: subcorpus de referință, până la 2020-01-30</catDesc>
      <catDesc xml:lang="en">
         <term>Reference</term>: reference subcorpus, until 2020-01-30</catDesc>
   </category>
   <category xml:id="covid">
      <catDesc xml:lang="ro">
         <term>COVID</term>: Subcorpus COVID, începând cu 2020-01-31, când OMS a făcut declarația oficială a Urgenței de sănătate publică de interes internațional pentru COVID-19</catDesc>
      <catDesc xml:lang="en">
         <term>COVID</term>: COVID subcorpus, from 2020-01-31 onwards, when WHO made the formal declaration of PHEIC, i.e. the Public Health Emergency of International Concern for COVID-19</catDesc>
   </category>
   <category xml:id="war">
      <catDesc xml:lang="ro">
         <term>Război</term>: Subcorpusul pentru războiul în Ucraina, începând cu 2022-02-24, adică data invaziei Rusiei în Ucraina</catDesc>
      <catDesc xml:lang="en">
         <term>War</term>: War in Ukraine subcorpus, from 2022-02-24 onwards, i.e. from Russia's full-scale invasion of Ukraine</catDesc>
   </category>
</taxonomy>
