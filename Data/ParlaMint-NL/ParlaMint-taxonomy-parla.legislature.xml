                  </category>
                  <category xml:id="parla.meeting.opinions">
                     <catDesc xml:lang="en">
                        <term>Public presentation of opinions</term>
                     </catDesc>
                     <catDesc xml:lang="nl">
                        <term>Publieke presentatie van meningen</term>
                     </catDesc>
                  </category>
               </category>
               <category xml:id="parla.meeting.continued">
                  <catDesc xml:lang="en">
                     <term>Continued meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nl">
                     <term>Voortgezette vergadering</term>
