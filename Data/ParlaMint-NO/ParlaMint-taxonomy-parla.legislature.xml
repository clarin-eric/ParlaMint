<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-parla.legislature"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Legislature</term>
   </desc>
   <desc xml:lang="nb">
      <term>Lovgivende forsamling</term>
   </desc>
   <category xml:id="parla.geo-political">
      <catDesc xml:lang="en">
         <term>Geo-political or administrative units</term>
      </catDesc>
      <catDesc xml:lang="nb">
         <term>Geo-politisk eller administrative enheter</term>
      </catDesc>
      <category xml:id="parla.supranational">
         <catDesc xml:lang="en">
            <term>Supranational legislature</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Overnasjonal lovgivende forsamling</term>
         </catDesc>
      </category>
      <category xml:id="parla.national">
         <catDesc xml:lang="en">
            <term>National legislature</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Nasjonal lovgivende forsamling</term>
         </catDesc>
      </category>
      <category xml:id="parla.regional">
         <catDesc xml:lang="en">
            <term>Regional legislature</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Regional lovgivende forsamling</term>
         </catDesc>
      </category>
      <category xml:id="parla.local">
         <catDesc xml:lang="en">
            <term>Local legislature</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Lokal lovgivende forsamling</term>
         </catDesc>
      </category>
   </category>
   <category xml:id="parla.organization">
      <catDesc xml:lang="en">
         <term>Organization</term>
      </catDesc>
      <catDesc xml:lang="nb">
         <term>Organisasjon</term>
      </catDesc>
      <category xml:id="parla.chambers">
         <catDesc xml:lang="en">
            <term>Chambers</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Kamre</term>
         </catDesc>
         <category xml:id="parla.uni">
            <catDesc xml:lang="en">
               <term>Ettkammersystem</term>
            </catDesc>
         </category>
         <category xml:id="parla.bi">
            <catDesc xml:lang="en">
               <term>Tokammersystem</term>
            </catDesc>
            <category xml:id="parla.upper">
               <catDesc xml:lang="en">
                  <term>Upper house</term>
               </catDesc>
               <catDesc xml:lang="nb">
                  <term>Overhus</term>
               </catDesc>
            </category>
            <category xml:id="parla.lower">
               <catDesc xml:lang="en">
                  <term>Lower house</term>
               </catDesc>
               <catDesc xml:lang="nb">
                  <term>Underhus</term>
               </catDesc>
            </category>
         </category>
         <category xml:id="parla.multi">
            <catDesc xml:lang="en">
               <term>Multicameralism</term>
            </catDesc>
            <catDesc xml:lang="nb">
               <term>Flerkammersystem</term>
            </catDesc>
            <category xml:id="parla.chamber">
               <catDesc xml:lang="en">
                  <term>Chamber</term>
               </catDesc>
               <catDesc xml:lang="nb">
                  <term>Kammer</term>
               </catDesc>
            </category>
         </category>
      </category>
      <category xml:id="parla.committee">
         <catDesc xml:lang="en">
            <term>Committee</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Kammer</term>
         </catDesc>
         <catDesc xml:lang="nb">
            <term>Komité</term>
         </catDesc>
         <category xml:id="parla.committee.standing">
            <catDesc xml:lang="en">
               <term>Standing committee</term>
            </catDesc>
            <catDesc xml:lang="nb">
               <term>Stående komité</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.special">
            <catDesc xml:lang="en">
               <term>Special committee</term>
            </catDesc>
            <catDesc xml:lang="nb">
               <term>Spesialkomité</term>
            </catDesc>
         </category>
         <category xml:id="parla.committee.inquiry">
            <catDesc xml:lang="en">
               <term>Committee of inquiry</term>
            </catDesc>
            <catDesc xml:lang="nb">
               <term>Undersøkelseskomité</term>
            </catDesc>
         </category>
      </category>
   </category>
   <category xml:id="parla.term">
      <catDesc xml:lang="en">
         <term>Legislative period</term>: term of the parliament between general elections.</catDesc>
      <catDesc xml:lang="nb">
         <term>Stortingsperiode</term>: Stortingets funksjonstid mellom valg.</catDesc>
      <category xml:id="parla.session">
         <catDesc xml:lang="en">
            <term>Session</term>: A parliamentary year, which always begins on the first working day in October at and end on the last working day of September.</catDesc>
         <catDesc xml:lang="nb">
            <term>Sesjon</term>: Et parlamentarisk år, som alltid begynner første arbeidsdag i oktober og slutter siste arbeidsdag i september</catDesc>
         <category xml:id="parla.meeting">
            <catDesc xml:lang="en">
               <term>Meeting</term>: Each meeting may be a separate session or part of a group of meetings constituting a session. The session/meeting may take one or more days.</catDesc>
            <catDesc xml:lang="nb">
               <term>Møte</term>: Hvert møte kan være en separat sesjon eller del av en gruppe møter som utgjør en sesjon. Sesjonen/møtet kan ta én eller flere dager.</catDesc>
            <category xml:id="parla.meeting-types">
               <catDesc xml:lang="en">
                  <term>Types of meetings</term>
               </catDesc>
               <catDesc xml:lang="nb">
                  <term>Typer møter</term>
               </catDesc>
               <category xml:id="parla.meeting.regular">
                  <catDesc xml:lang="en">
                     <term>Regular meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nb">
                     <term>Regulært møte</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.special">
                  <catDesc xml:lang="en">
                     <term>Special meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nb">
                     <term>Spesialmøte</term>
                  </catDesc>
                  <category xml:id="parla.meeting.extraordinary">
                     <catDesc xml:lang="en">
                        <term>Extraordinary meeting</term>
                     </catDesc>
                     <catDesc xml:lang="nb">
                        <term>Ekstraordinært møte</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.urgent">
                     <catDesc xml:lang="en">
                        <term>Urgent meeting</term>
                     </catDesc>
                     <catDesc xml:lang="nb">
                        <term>Hastemøte</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.ceremonial">
                     <catDesc xml:lang="en">
                        <term>Ceremonial meeting</term>
                     </catDesc>
                     <catDesc xml:lang="nb">
                        <term>Seremonielt møte</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.commemorative">
                     <catDesc xml:lang="en">
                        <term>Commemorative meeting</term>
                     </catDesc>
                     <catDesc xml:lang="nb">
                        <term>Minnemøte</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.opinions">
                     <catDesc xml:lang="en">
                        <term>Public presentation of opinions</term>
                     </catDesc>
                     <catDesc xml:lang="nb">
                        <term>Offentlig presentasjon av meninger</term>
                     </catDesc>
                  </category>
               </category>
               <category xml:id="parla.meeting.continued">
                  <catDesc xml:lang="en">
                     <term>Continued meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nb">
                     <term>Fortsatt møte</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.public">
                  <catDesc xml:lang="en">
                     <term>Public meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nb">
                     <term>Offentlig møte</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.executive">
                  <catDesc xml:lang="en">
                     <term>Executive meeting</term>
                  </catDesc>
                  <catDesc xml:lang="nb">
                     <term>Beslutningsmøte</term>
                  </catDesc>
               </category>
            </category>
            <category xml:id="parla.sitting">
               <catDesc xml:lang="en">
                  <term>Sitting</term>: a parliamentary meeting. The course of the year is planned in accordance with a number of working processes and recurring events. Sittings for the coming parliamentary year are planned in advance, and the plan specifies the days on which there will be sittings and the weeks during which there will be no sittings in the Chamber. Parliamentary committees usually have one or two regular weekly meetings, but not all of them hold meetings every week, especially not during the summer. During a parliamentary year, there are about 100 sitting days.</catDesc>
               <catDesc xml:lang="nb">
                  <term>Møtedag</term> : et parlamentarisk møte.</catDesc>
            </category>
         </category>
      </category>
   </category>
</taxonomy>
