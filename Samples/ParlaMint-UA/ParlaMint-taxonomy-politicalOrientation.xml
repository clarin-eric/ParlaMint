<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-politicalOrientation"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Political orientation of political parties and parliamentary groups</term>
   </desc>
   <desc xml:lang="uk">
      <term>політична орієнтація партій і парламентських груп</term>
   </desc>
   <category xml:id="orientation.L">
      <catDesc xml:lang="en">
         <term>Left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>ліві</term>
      </catDesc>
   </category>
   <category xml:id="orientation.C">
      <catDesc xml:lang="en">
         <term>Centre</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>центристи</term>
      </catDesc>
   </category>
   <category xml:id="orientation.R">
      <catDesc xml:lang="en">
         <term>Right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>праві</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FL">
      <catDesc xml:lang="en">
         <term>Far-left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>ліворадикали</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FR">
      <catDesc xml:lang="en">
         <term>Far-right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>праворадикали</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CL">
      <catDesc xml:lang="en">
         <term>Centre-left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>лівоцентристи</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CR">
      <catDesc xml:lang="en">
         <term>Centre-right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>правоцентристи</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCL">
      <catDesc xml:lang="en">
         <term>Centre to centre-left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>центристи — лівоцентристи</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCR">
      <catDesc xml:lang="en">
         <term>Centre to centre-right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>центристи — правоцентристи</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CLL">
      <catDesc xml:lang="en">
         <term>Centre-left to left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>лівоцентристи — ліві</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CRR">
      <catDesc xml:lang="en">
         <term>Centre-right to right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>правоцентристи — праві</term>
      </catDesc>
   </category>
   <category xml:id="orientation.LLF">
      <catDesc xml:lang="en">
         <term>Left to far-left</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>ліві — ліворадикали</term>
      </catDesc>
   </category>
   <category xml:id="orientation.RRF">
      <catDesc xml:lang="en">
         <term>Right to far-right</term>
      </catDesc>
      <catDesc xml:lang="uk">
         <term>праві — праворадикали</term>
      </catDesc>
   </category>
   <category xml:id="orientation.BT">
      <catDesc xml:lang="en">
         <term>Big tent</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Big tent</ref> or catch-all refers to political parties that have members covering a broad spectrum of beliefs.</catDesc>
      <catDesc xml:lang="uk">
         <term>універсальні партії</term>
      </catDesc>
   </category>
   <category xml:id="orientation.NP">
      <catDesc xml:lang="en">
         <term>Nonpartisanism</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Nonpartisanism</ref> refers to a political stance that does not agree with the current political party system.</catDesc>
      <catDesc xml:lang="uk">
         <term>понадпартійність</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Понадпартійність</ref>
         вказує на політичну позицію, яка не відповідає поточній системі політичних партій
      </catDesc>
   </category>
   <category xml:id="orientation.PP">
      <catDesc xml:lang="en">
         <term>Pirate Party</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Pirate Party</ref> refers to political parties that support civil rights, direct democracy, encourage innovation and creativity, free sharing of knowledge, information privacy, free speech, anti-corruption, net neutrality and oppose mass surveillance, censorship and Big Tech.</catDesc>
      <catDesc xml:lang="uk">
         <term>піратська партія</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Піратські партії</ref>
         це політичні партії, які підтримують громадянські права, пряму демократію, заохочують інновації та творчість, вільний обмін знаннями, конфіденційність інформації, свободу слова, боротьбу з корупцією, мережевий нейтралітет, а також виступають проти масового стеження, цензури та технологічних компаній-гігантів.
      </catDesc>
   </category>
   <category xml:id="orientation.SI">
      <catDesc xml:lang="en">
         <term>Single Issue Politics</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Single Issue Politics</ref> refers to a political stance that is based on one essential policy area or idea.</catDesc>
      <catDesc xml:lang="uk">
         <term>партія одного питання</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Партія одного питання</ref> це партія, діяльність якої зосереджена на одній основній сфері політики чи одній ідеї.
      </catDesc>
   </category>
   <category xml:id="orientation.SY">
      <catDesc xml:lang="en">
         <term>Syncretic politics</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Syncretic politics</ref> refers to politics that combine elements from across the conventional left–right political spectrum.</catDesc>
      <catDesc xml:lang="uk">
         <term>синкретична політика</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Синкретична політика</ref> це політика, яка поєднує елементи із загальноприйнятих лівого і правого політичних спектрів.
      </catDesc>
   </category>
</taxonomy>
