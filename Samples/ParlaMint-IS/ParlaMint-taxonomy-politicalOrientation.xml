<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-politicalOrientation"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Political orientation of political parties and parliamentary groups</term>
   </desc>
   <desc xml:lang="is">
      <term>Pólitísk stefnumörkun stjórnmálaflokka og þingflokka</term>
   </desc>
   <category xml:id="orientation.L">
      <catDesc xml:lang="en">
         <term>Left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.C">
      <catDesc xml:lang="en">
         <term>Centre</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Miðja</term>
      </catDesc>
   </category>
   <category xml:id="orientation.R">
      <catDesc xml:lang="en">
         <term>Right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FL">
      <catDesc xml:lang="en">
         <term>Far-left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Yst til vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FR">
      <catDesc xml:lang="en">
         <term>Far-right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Yst til hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CL">
      <catDesc xml:lang="en">
         <term>Centre-left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Mið-vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CR">
      <catDesc xml:lang="en">
         <term>Centre-right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Mið-hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCL">
      <catDesc xml:lang="en">
         <term>Centre to centre-left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Miðja til mið-vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCR">
      <catDesc xml:lang="en">
         <term>Centre to centre-right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Miðja til mið-hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CLL">
      <catDesc xml:lang="en">
         <term>Centre-left to left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Mið-vinstri til vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CRR">
      <catDesc xml:lang="en">
         <term>Centre-right to right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Mið-hægri til hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.LLF">
      <catDesc xml:lang="en">
         <term>Left to far-left</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Vinstri til yst til vinstri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.RRF">
      <catDesc xml:lang="en">
         <term>Right to far-right</term>
      </catDesc>
      <catDesc xml:lang="is">
         <term>Hægri til yst til hægri</term>
      </catDesc>
   </category>
   <category xml:id="orientation.BT">
      <catDesc xml:lang="en">
         <term>Big tent</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Big tent</ref> or catch-all refers to political parties that have members covering a broad spectrum of beliefs.</catDesc>
      <catDesc xml:lang="is">
         <term>Breiðflokkur</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Breiðflokkur</ref>
         vísar til stjórnmálaflokka þar sem meðlimir hafa fjölbreytta sannfæringu og trú
      </catDesc>
   </category>
   <category xml:id="orientation.NP">
      <catDesc xml:lang="en">
         <term>Nonpartisanism</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Nonpartisanism</ref> refers to a political stance that does not agree with the current political party system.</catDesc>
      <catDesc xml:lang="is">
         <term>Flokksleysi</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Flokksleysi</ref>
         vísar til þeirrar pólítísku afstöðu að vera ósammála núverandi flokkskerfi
      </catDesc>
   </category>
   <category xml:id="orientation.PP">
      <catDesc xml:lang="en">
         <term>Pirate Party</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Pirate Party</ref> refers to political parties that support civil rights, direct democracy, encourage innovation and creativity, free sharing of knowledge, information privacy, free speech, anti-corruption, net neutrality and oppose mass surveillance, censorship and Big Tech.</catDesc>
      <catDesc xml:lang="is">
         <term>Pírataflokkur</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Pírataflokkur</ref>
         vísar til stjórnmálaflokka sem styðja borgararéttindi, beint lýðræði, hvetja til nýsköpunar og sköpunargáfu, frjálsa miðlun þekkingar, persónuvernd upplýsinga, tjáningarfrelsis, baráttu gegn spillingu, nethlutleysi og eru á móti fjöldaeftirliti, ritskoðun og Big Tech.
      </catDesc>
   </category>
   <category xml:id="orientation.SI">
      <catDesc xml:lang="en">
         <term>Single Issue Politics</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Single Issue Politics</ref> refers to a political stance that is based on one essential policy area or idea.</catDesc>
      <catDesc xml:lang="is">
         <term>Einsmálsflokkur</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Einsmálsflokkur</ref>
         vísar til pólitískrar afstöðu sem byggir á einu mikilvægu stefnumáli eða hugmynd.
      </catDesc>
   </category>
   <category xml:id="orientation.SY">
      <catDesc xml:lang="en">
         <term>Syncretic politics</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Syncretic politics</ref> refers to politics that combine elements from across the conventional left–right political spectrum.</catDesc>
      <catDesc xml:lang="is">
         <term>Synkretísk stjórnmál</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Synkretísk stjórnmál</ref>
         vísar til stjórnmála sem sameina þætti frá hinum hefðbundna vinstri-hægri stjórnmálasviði.
      </catDesc>
   </category>
</taxonomy>
