<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-politicalOrientation"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Political orientation of political parties and parliamentary groups</term>
   </desc>
   <desc xml:lang="hu">
      <term>Politikai pártok és parlamenti frakciók politikai irányzatai</term>
   </desc>
   <category xml:id="orientation.L">
      <catDesc xml:lang="en">
         <term>Left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Baloldal</term>
      </catDesc>
   </category>
   <category xml:id="orientation.C">
      <catDesc xml:lang="en">
         <term>Centre</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Közép</term>
      </catDesc>
   </category>
   <category xml:id="orientation.R">
      <catDesc xml:lang="en">
         <term>Right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Jobboldal</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FL">
      <catDesc xml:lang="en">
         <term>Far-left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Szélsőbal</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FR">
      <catDesc xml:lang="en">
         <term>Far-right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Szélsőjobb</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CL">
      <catDesc xml:lang="en">
         <term>Centre-left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Balközép</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CR">
      <catDesc xml:lang="en">
         <term>Centre-right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Jobbközép</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCL">
      <catDesc xml:lang="en">
         <term>Centre to centre-left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Középtől balközépig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCR">
      <catDesc xml:lang="en">
         <term>Centre to centre-right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>Középtől jobbközépig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CLL">
      <catDesc xml:lang="en">
         <term>Centre-left to left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>A balközéptől a baloldalig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CRR">
      <catDesc xml:lang="en">
         <term>Centre-right to right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>A jobbközéptől a jobboldalig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.LLF">
      <catDesc xml:lang="en">
         <term>Left to far-left</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>A baloldaltól a szélsőbalig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.RRF">
      <catDesc xml:lang="en">
         <term>Right to far-right</term>
      </catDesc>
      <catDesc xml:lang="hu">
         <term>A jobboldaltól a szélsőjobbig</term>
      </catDesc>
   </category>
   <category xml:id="orientation.BT">
      <catDesc xml:lang="en">
         <term>Big tent</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Big tent</ref> or catch-all refers to political parties that have members covering a broad spectrum of beliefs.</catDesc>
      <catDesc xml:lang="hu">
         <term>Gyűjtőpártok</term>: <ref target="https://hu.m.wikipedia.org/wiki/Gy%C5%B1jt%C5%91p%C3%A1rt">A gyűjtőpártok</ref> vagy néppártok olyan pártok, amelyeknek a tagjai a politikai meggyőződések széles spektrumát lefedik.</catDesc>
   </category>
   <category xml:id="orientation.NP">
      <catDesc xml:lang="en">
         <term>Nonpartisanism</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Nonpartisanism</ref> refers to a political stance that does not agree with the current political party system.</catDesc>
      <catDesc xml:lang="hu">
         <term>Pártsemlegesség</term>: A pártsemlegesség vagy pártonkívüliség olyan politikai álláspontra utal, amely nem ért egyet a jelenlegi politikai pártrendszerrel.</catDesc>
   </category>
   <category xml:id="orientation.PP">
      <catDesc xml:lang="en">
         <term>Pirate Party</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Pirate Party</ref> refers to political parties that support civil rights, direct democracy, encourage innovation and creativity, free sharing of knowledge, information privacy, free speech, anti-corruption, net neutrality and oppose mass surveillance, censorship and Big Tech.</catDesc>
      <catDesc xml:lang="hu">
         <term>Kalózpárt</term>: A kalózpárt elnevezés olyan politikai pártokra utal, amelyek támogatják a polgári jogokat, a közvetlen demokráciát, ösztönzik az innovációt és a kreativitást, a tudás szabad megosztását, a magánélet védelmét, a szólásszabadságot, a korrupcióellenességet, a hálózatsemlegességet, és ellenzik a tömeges megfigyelést, a cenzúrát és a Big Tech-eket.</catDesc>
   </category>
   <category xml:id="orientation.SI">
      <catDesc xml:lang="en">
         <term>Single Issue Politics</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Single Issue Politics</ref> refers to a political stance that is based on one essential policy area or idea.</catDesc>
      <catDesc xml:lang="hu">
         <term>Egytémás politika</term>: olyan politikai álláspontra utal, amely egyetlen alapvető politikai területre vagy elképzelésre épül.</catDesc>
   </category>
   <category xml:id="orientation.SY">
      <catDesc xml:lang="en">
         <term>Syncretic politics</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Syncretic politics</ref> refers to politics that combine elements from across the conventional left–right political spectrum.</catDesc>
      <catDesc xml:lang="hu">
         <term>Szinkretista politika</term>: az eltérő vagy ellentmondó, jobb- vagy baloldali nézetek összeegyeztetésére irányuló törekvés.</catDesc>
   </category>
</taxonomy>
