   <category xml:id="parla.term">
      <catDesc xml:lang="en">
         <term>Legislative period</term>: term of the parliament between general elections.</catDesc>
      <catDesc xml:lang="da">
         <term>Valgperiode</term>: periode mellem to valg.</catDesc>
      <category xml:id="parla.session">
         <catDesc xml:lang="en">
            <term>Legislative session</term>: the period of time in which a legislature is convened for
      purpose of lawmaking, usually being one of two or more smaller divisions of the entire time between two elections.
      A session is a meeting or series of connected meetings devoted to a single order of business, program, agenda, or
      announced purpose.</catDesc>
         <catDesc xml:lang="da">
            <term>Folketingsår</term>: Valgperioden inddeles i folketingsår. Folketingsåret begynder den første tirsdag i oktober og varer til den første tirsdag i oktober det følgende år. Når der har været folketingsvalg, deles folketingsåret i to samlinger. Folketingsåret består af en række møder der følger en forudbestemt dagsorden.</catDesc>
         <category xml:id="parla.meeting">
            <catDesc xml:lang="en">
               <term>Meeting</term>: Each meeting may be a separate session or part of a group of meetings
	constituting a session. The session/meeting may take one or more days.</catDesc>
            <catDesc xml:lang="da">
               <term>Møde</term>: Folketingssamlinger består af en række møder. Et møde kan tage et aller flere dage.</catDesc>
            <category xml:id="parla.meeting-types">
               <catDesc xml:lang="en">
                  <term>Types of meetings</term>
               </catDesc>
               <catDesc xml:lang="da">
                  <term>Mødetyper</term>
               </catDesc>
               <category xml:id="parla.meeting.regular">
                  <catDesc xml:lang="en">
                     <term>Regular meeting</term>
                  </catDesc>
                  <catDesc xml:lang="da">
                     <term>Almindeligt møde</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.special">
                  <catDesc xml:lang="en">
                     <term>Special meeting</term>
                  </catDesc>
                  <catDesc xml:lang="da">
                     <term>Specielt møde</term>
                  </catDesc>
                  <category xml:id="parla.meeting.extraordinary">
                     <catDesc xml:lang="en">
                        <term>Extraordinary meeting</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Ekstraordinært møde</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.urgent">
                     <catDesc xml:lang="en">
                        <term>Urgent meeting</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Hastemøde</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.ceremonial">
                     <catDesc xml:lang="en">
                        <term>Ceremonial meeting</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Ceremonielt møde</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.commemorative">
                     <catDesc xml:lang="en">
                        <term>Commemorative meeting</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Mindemøde</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.opinions">
                     <catDesc xml:lang="en">
                        <term>Public presentation of opinions</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Offentlig fremlæggelse af udtalelser</term>
                     </catDesc>
                  </category>
                  <category xml:id="parla.meeting.committee">
                     <catDesc xml:lang="en">
                        <term>Committee meeting</term>
                     </catDesc>
                     <catDesc xml:lang="da">
                        <term>Udvalgsmøder</term>
                     </catDesc>
                  </category>
               </category>
               <category xml:id="parla.meeting.continued">
                  <catDesc xml:lang="en">
                     <term>Continued meeting</term>
                  </catDesc>
                  <catDesc xml:lang="da">
                     <term>Fortsat møde</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.public">
                  <catDesc xml:lang="en">
                     <term>Public meeting</term>
                  </catDesc>
                  <catDesc xml:lang="da">
                     <term>Offentligt møde</term>
                  </catDesc>
               </category>
               <category xml:id="parla.meeting.executive">
                  <catDesc xml:lang="en">
                     <term>Executive meeting</term>
                  </catDesc>
                  <catDesc xml:lang="da">
                     <term>Direktionsmøde</term>
                  </catDesc>
               </category>
            </category>
            <category xml:id="parla.sitting">
               <catDesc xml:lang="en">
                  <term>Sitting</term>: sitting day</catDesc>
               <catDesc xml:lang="da">
                  <term>Folketingsmøde</term>: mødedag i Folketinget</catDesc>
            </category>
         </category>
      </category>
   </category>
</taxonomy>
