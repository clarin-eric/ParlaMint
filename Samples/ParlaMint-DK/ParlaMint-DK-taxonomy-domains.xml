<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0" xml:id="ParlaMint-DK-taxonomy-domains" xml:lang="mul">
   <desc xml:lang="en"><term>Policy domains</term> in the ParlaMint-DK corpus</desc>
   <desc xml:lang="da"><term>Politiske emneområder</term> i ParlaMint-DK</desc>
   <category xml:id="domain.Agriculture">
      <catDesc xml:lang="en"><term>Agriculture</term>: comprises Agriculture, Fisheries, Food, Consumer, and Animal welfare</catDesc>
      <catDesc xml:lang="da"><term>Landbrug</term>: omfatter Landbrug, Fiskeri, Fødevarer, Forbruger, og Dyrevelfærd</catDesc>
   </category>
   <category xml:id="domain.Business">
      <catDesc xml:lang="en"><term>Business and commerce</term></catDesc>
      <catDesc xml:lang="da"><term>Erhverv</term></catDesc>
   </category>
   <category xml:id="domain.Culture">
      <catDesc xml:lang="en"><term>Culture</term>: comprises Culture, Churches and religious communities, and Sports</catDesc>
      <catDesc xml:lang="da"><term>Kultur</term>: omfatter Kultur, Kirke, og Idræt</catDesc>
   </category>
   <category xml:id="domain.Defence">
      <catDesc xml:lang="en"><term>Defence</term></catDesc>
      <catDesc xml:lang="da"><term>Forsvar</term></catDesc>
   </category>
   <category xml:id="domain.Economy">
      <catDesc xml:lang="en"><term>Finance</term>: comprises Finance and Taxation</catDesc>
      <catDesc xml:lang="da"><term>Finans</term>: omfatter Finans og Skat</catDesc>
   </category>
   <category xml:id="domain.Education">
      <catDesc xml:lang="en"><term>Education and research</term>: comprises Education and research</catDesc>
      <catDesc xml:lang="da"><term>Uddannelse og forskning</term>: omfatter Uddannelse og forskning samt Undervisning</catDesc>
   </category>
   <category xml:id="domain.Energy">
      <catDesc xml:lang="en"><term>Energy</term>: comprises Energy and Climate</catDesc>
      <catDesc xml:lang="da"><term>Energi</term>: omfatter Energi og Klima</catDesc>
   </category>
   <category xml:id="domain.Environment">
      <catDesc xml:lang="en"><term>Environment</term></catDesc>
      <catDesc xml:lang="da"><term>Miljø</term></catDesc>
   </category>
   <category xml:id="domain.Europe">
      <catDesc xml:lang="en"><term>European affairs</term></catDesc>
      <catDesc xml:lang="da"><term>EU</term></catDesc>
   </category>
   <category xml:id="domain.Foreign">
      <catDesc xml:lang="en"><term>Foreign affairs</term>: comprises Foreign affairs, and Foreign Aid</catDesc>
      <catDesc xml:lang="da"><term>Udenrigsanliggender</term>: omfatter Udenrigsanliggender og Udvikling</catDesc>
   </category>
   <category xml:id="domain.Health">
      <catDesc xml:lang="en"><term>Health care</term>: comprises Health and Sickness, and Psychiatry</catDesc>
      <catDesc xml:lang="da"><term>Sundhed</term>: omfatter Sundhed og Psykiatri</catDesc>
   </category>
   <category xml:id="domain.Housing">
      <catDesc xml:lang="en"><term>Housing and building</term></catDesc>
      <catDesc xml:lang="da"><term>Bolig</term></catDesc>
   </category>
   <category xml:id="domain.Immigration">
      <catDesc xml:lang="en"><term>Immigration</term>: comprises Integration, Immigration, and Naturalization</catDesc>
      <catDesc xml:lang="da"><term>Udlænding og integration</term>: omfatter Integration, Udlænding og Indfødsret</catDesc>
   </category>
   <category xml:id="domain.Infrastructure">
      <catDesc xml:lang="en"><term>Infrastructure</term>: comprises Traffic and transport, IT and Media</catDesc>
      <catDesc xml:lang="da"><term>Transport</term>: omfatter Transport, IT og Medie</catDesc>
   </category>
   <category xml:id="domain.Justice">
      <catDesc xml:lang="en"><term>Justice</term>: comprises Justice and the law courts, and Costitution</catDesc>
      <catDesc xml:lang="da"><term>Justits</term>: omfatter Ret og Grundlov</catDesc>
   </category>
   <category xml:id="domain.Labour">
      <catDesc xml:lang="en"><term>Labor and employment</term></catDesc>
      <catDesc xml:lang="da"><term>Beskæftigelse</term></catDesc>
   </category>
   <category xml:id="domain.Local">
      <catDesc xml:lang="en"><term>Domestic affairs</term>: comprises Domestic Affairs, and Rural Districts and Islands</catDesc>
      <catDesc xml:lang="da"><term>Inderigsanliggender</term>: omfatter Kommuner og Landdistrikter</catDesc>
   </category>
   <category xml:id="domain.Social">
      <catDesc xml:lang="en"><term>Social affairs</term>: comprises Children, Family, Disabled, Social conditions and wellfare, Senior Citizens, and Gender equality</catDesc>
      <catDesc xml:lang="da"><term>Sociale anliggenheder</term>: omfatter Børn, Familie, Handicap, Sociale anliggender, Ældre og Ligestilling</catDesc>
   </category>
   <category xml:id="domain.Territories">
      <catDesc xml:lang="en"><term>Greenland and the Faroe Islands</term></catDesc>
      <catDesc xml:lang="da"><term>Grønland og Færøerne</term></catDesc>
   </category>
   <category xml:id="domain.other">
      <catDesc xml:lang="en"><term>Government operations</term></catDesc>
      <catDesc xml:lang="da"><term>Folketingsanliggender</term></catDesc>
   </category>
</taxonomy>

