<?xml version="1.0" encoding="UTF-8"?>
<taxonomy xmlns="http://www.tei-c.org/ns/1.0"
          xml:id="ParlaMint-taxonomy-politicalOrientation"
          xml:lang="mul">
   <desc xml:lang="en">
      <term>Political orientation of political parties and parliamentary groups</term>
   </desc>
   <desc xml:lang="da">
      <term>Partier og parlamentariske gruppers politiske orientering</term>
   </desc>
   <category xml:id="orientation.L">
      <catDesc xml:lang="en">
         <term>Left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.C">
      <catDesc xml:lang="en">
         <term>Centre</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum</term>
      </catDesc>
   </category>
   <category xml:id="orientation.R">
      <catDesc xml:lang="en">
         <term>Right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FL">
      <catDesc xml:lang="en">
         <term>Far-left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Yderst til venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.FR">
      <catDesc xml:lang="en">
         <term>Far-right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Yderst til højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CL">
      <catDesc xml:lang="en">
         <term>Centre-left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum-venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CR">
      <catDesc xml:lang="en">
         <term>Centre-right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum-højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCL">
      <catDesc xml:lang="en">
         <term>Centre to centre-left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum til centrum-venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CCR">
      <catDesc xml:lang="en">
         <term>Centre to centre-right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum til centrum-højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CLL">
      <catDesc xml:lang="en">
         <term>Centre-left to left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum-venstre til venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.CRR">
      <catDesc xml:lang="en">
         <term>Centre-right to right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Centrum-højre til højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.LLF">
      <catDesc xml:lang="en">
         <term>Left to far-left</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Venstre til yderste venstre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.RRF">
      <catDesc xml:lang="en">
         <term>Right to far-right</term>
      </catDesc>
      <catDesc xml:lang="da">
         <term>Højre til yderste højre</term>
      </catDesc>
   </category>
   <category xml:id="orientation.BT">
      <catDesc xml:lang="en">
         <term>Big tent</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Big tent</ref> or catch-all refers to political parties that have members covering a broad spectrum of beliefs.
      </catDesc>
      <catDesc xml:lang="da">
         <term>Big tent</term>: <ref target="https://en.wikipedia.org/wiki/Big_tent">Big tent</ref>
         eller catch all partier refererer til politiske partier hvis medlemmer har en bred vifte af overbevisninger.
      </catDesc>
   </category>
   <category xml:id="orientation.NP">
      <catDesc xml:lang="en">
         <term>Nonpartisanism</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Nonpartisanism</ref> refers to a political stance that does not agree with the current political party system.</catDesc>
      <catDesc xml:lang="da">
         <term>Partiløs demokrati</term>: <ref target="https://en.wikipedia.org/wiki/Nonpartisanism">Partiløs demokrati</ref>
         henviser til en politisk holdning, der ikke stemmer overens med det nuværende politiske partisystem.
      </catDesc>
   </category>
   <category xml:id="orientation.PP">
      <catDesc xml:lang="en">
         <term>Pirate Party</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Pirate Party</ref> refers to political parties that support civil rights, direct democracy, encourage innovation and creativity, free sharing of knowledge, information privacy, free speech, anti-corruption, net neutrality and oppose mass surveillance, censorship and Big Tech.</catDesc>
      <catDesc xml:lang="da">
         <term>Piratparti</term>: <ref target="https://en.wikipedia.org/wiki/Pirate_Party">Piratparti</ref>
         henviser til politiske partier, der støtter borgerrettigheder, direkte demokrati, tilskynder til innovation og kreativitet, fri deling af viden, informationsbeskyttelse, ytringsfrihed, anti-korruption, netneutralitet og modsætter sig masseovervågning, censur og Big Tech. 
      </catDesc>
   </category>
   <category xml:id="orientation.SI">
      <catDesc xml:lang="en">
         <term>Single Issue Politics</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Single Issue Politics</ref> refers to a political stance that is based on one essential policy area or idea.</catDesc>
      <catDesc xml:lang="da">
         <term>Enkeltsagspolitik</term>: <ref target="https://en.wikipedia.org/wiki/Single-issue_politics">Enkeltsagspolitik</ref>
         henviser til en politisk holdning, der er baseret på ét væsentligt politisk område eller en idé.
      </catDesc>
   </category>
   <category xml:id="orientation.SY">
      <catDesc xml:lang="en">
         <term>Syncretic politics</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Syncretic politics</ref> refers to politics that combine elements from across the conventional left–right political spectrum.</catDesc>
      <catDesc xml:lang="da">
         <term>Synkretisk politik</term>: <ref target="https://en.wikipedia.org/wiki/Syncretic_politics">Synkretisk politik</ref>
         refererer til politik, der kombinerer elementer fra hele det konventionelle venstre-højre politiske spektrum.
      </catDesc>
   </category>
</taxonomy>
