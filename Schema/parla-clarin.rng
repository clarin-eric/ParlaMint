            <zeroOrMore>
               <ref name="tei_model.headLike"/>
            </zeroOrMore>
      
            <choice>
        
          
               <zeroOrMore>
                  <ref name="tei_model.pLike"/>
               </zeroOrMore>
          
        
        
               <zeroOrMore>
                  <choice>
                     <ref name="tei_model.labelLike"/>
                     <ref name="tei_model.placeStateLike"/>
                     <ref name="tei_model.eventLike"/>
