            <choice>
               <oneOrMore>
                  <group>
                     <ref name="tei_row"/>
          
                     <zeroOrMore>
                        <ref name="tei_model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
               <oneOrMore>
                  <group>
          
                     <ref name="tei_model.graphicLike"/>
          
          
                     <zeroOrMore>
                        <ref name="tei_model.global"/>
                     </zeroOrMore>
          
                  </group>
               </oneOrMore>
            </choice>
            <zeroOrMore>
               <group>
        
                  <ref name="tei_model.divBottom"/>
        
        
                  <zeroOrMore>
                     <ref name="tei_model.global"/>
                  </zeroOrMore>
        
               </group>
            </zeroOrMore>
         </group>
         <ref name="tei_att.global.attributes"/>
         <ref name="tei_att.typed.attributes"/>
